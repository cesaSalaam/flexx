	pod 'Bolts'
	pod 'Firebase/Database'
	pod 'FBSDKCoreKit'
	pod 'FBSDKLoginKit'
	pod 'FBSDKShareKit'
	pod 'KeyboardLayoutHelper'
	
  target 'FlexxRideTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FlexxRideUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
